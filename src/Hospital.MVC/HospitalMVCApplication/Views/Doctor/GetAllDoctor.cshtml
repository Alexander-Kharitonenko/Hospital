@model ViewModelForDoctor
@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer Localizer


@{
    ViewData["Title"] = "DoctorTable";
}

<div>

    <table class="table">
        <thead>
            <tr>
                <th scope="col">@Localizer["Id"]</th>
                <th scope="col">@Localizer["FirstName"]</th>
                <th scope="col">@Localizer["LastName"]</th>
                <th scope="col">@Localizer["Patronymic"]</</th>
                <th scope="col">@Localizer["NumberPhone"]</th>
                <th scope="col">@Localizer["Update"]</th>
                <th scope="col">@Localizer["Remove"]</th>
            </tr>
            @{
                foreach (var el in Model.AllDoctors)
                {

                    await Html.RenderPartialAsync("DoctorTable", el);
                }
            }

        </thead>

    </table>

</div>
