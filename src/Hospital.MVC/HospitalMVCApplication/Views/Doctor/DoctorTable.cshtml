@model Doctor

<tbody>
    <tr>
        <td>@Model.Id</td>
        <td>@Model.FirstName</td>
        <td>@Model.LastName</td>
        <td>@Model.Patronymic</td>
        <td>@Model.NumberPhone</td>
        <td><a class="btn btn-primary" asp-route-id="@Model.Id" asp-action="Edit" asp-controller="Doctor">Редактировать</a></td>
        <td><a class="btn btn-danger" asp-route-id="@Model.Id" asp-action="Remove" asp-controller="Doctor">Удалить</a></td>
    </tr>
</tbody>

