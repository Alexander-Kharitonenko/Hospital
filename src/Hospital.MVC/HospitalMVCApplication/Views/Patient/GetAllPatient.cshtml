@model ViewModelForPatient

@{
    ViewData["Title"] = "PatientTable";
}

<div>

    <table class="table">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">FirstName</th>
                <th scope="col">LastName</th>
                <th scope="col">Gender</th>
                <th scope="col">Patronymic</</th>
                <th scope="col">ResidenceAddress</</th>
                <th scope="col">Обновление</th>
                <th scope="col">Удаление</th>
            </tr>
            @{
                foreach (var el in Model.AllPatient)
                {
                    await Html.RenderPartialAsync("PatientTable", el);
                }
            }

        </thead>

    </table>

</div>