@model ViewModelForFilter

<div>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">@Model.NameFilter</th>
            </tr>
            @{
                foreach (var el in @Model.Filter)
                {
                    <tr>
                        <td>@el</td>
                    </tr>
                }
            }
        </thead>
    </table>
</div>

